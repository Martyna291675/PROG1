#include <stdio.h>

int main()
{
    int tab[10];
    int i;
    int rozmiar = sizeof(tab)/sizeof(int);
    
    for (i = 0 ; i < rozmiar ; i++)
        scanf("%d", &tab[i]);
    printf("Wpisz 0 dla FIFO; wpisz 1 dla LIFO\n");
    scanf("%d", &i);
    
    if (i == 0)
        for (i = 0 ; i < rozmiar ; i++)
            printf ("Tab[i]= %d\n", tab[i]);
        else if (i == 1)
            for (i = rozmiar-1 ; i >= 0 ; i--)
                printf ("Tab[%d]= %d\n", i, tab[i]);
        else
            printf("ZÅ‚e polecenie.\n");
    
    return 0;
}
